<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://xamarin.com/schemas/2014/forms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SpectrumTestApp.Views"
    Title="SpectrumTestApp"
    x:Class="SpectrumTestApp.AppShell">
    <TabBar>
        <ShellContent
            Title="Services"
            Icon="icon_feed.png"
            ContentTemplate="{DataTemplate local:ItemsPage}" />
        <ShellContent
            Title="About Me"
            Icon="icon_about.png"
            Route="AboutPage"
            ContentTemplate="{DataTemplate local:AboutPage}" />
    </TabBar>
</Shell>
